{
  "name": "js-chess-engine",
  "version": "2.0.1-rc.2",
  "description": "Simple and fast Node.js chess engine without dependencies",
  "keywords": [
    "chess",
    "game",
    "chess engine"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "play": "node example/play.mjs",
    "preversion": "npm test && npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "author": "bc.josefjadrny@gmail.com",
  "repository": "github:josefjadrny/js-chess-engine",
  "homepage": "https://github.com/josefjadrny/js-chess-engine#readme",
  "license": "MIT",
  "engines": {
    "node": ">=24"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "conventional-changelog-cli": "^5.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
